<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sun.newlanguage">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".NewApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.DataBindingActivity"></activity>

        <service
            android:name=".design.TimeService"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name=".activity.TransparentActivity"
            android:theme="@style/Transparent" />
        <activity android:name=".activity.CommonViewPageActivity" />
        <activity android:name=".activity.AddViewActivity" />
        <activity android:name=".activity.ModelActivity" />
        <activity android:name=".activity.SendActivity" />
        <activity
            android:name=".activity.CollapseActivity"
            android:theme="@style/NoActionBar" />
        <activity android:name=".activity.WebviewActivity" />
        <activity
            android:name=".activity.ViewPagerActivity"
            android:theme="@style/full" />
        <activity android:name=".activity.ProgressActivity" />
        <activity android:name=".activity.RotateActivity" />
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.AddActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>